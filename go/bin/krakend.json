{
  "version": 2,
   "timeout": "200000ms",
  "name": "{{ .service.name }}",
  "port": {{ .service.port }},
  "extra_config": {{ include "cors.txt" }},
  "endpoints": [
      {{ template "public_group" "/api/v1/registration"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/{username}"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/reset-password"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/user/streamers"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/file"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/login"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/refresh-token"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/logout"}}
      {{ template "backend" $AmazonCognitoAdapter}},

      {{ template "authorization_admin_group" "/api/v1/admin/grant-authority"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/users-in-group"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/disable-user"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/enable-user"}}
      {{ template "backend" $AmazonCognitoAdapter}},

      {{ template "public_group" "/api/v1/streams-page"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/streams/{streamId}"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/stream/close"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/streamers"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/streams/{streamId}/info"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/streams/{streamId}/owner"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/stream/decrease-viewers"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/stream/increase-views"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/stream/increase-viewers"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/stream"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/categories/{categoryId}"}}
      {{ template "backend" $StreamRegistry}},

      {{ template "authorization_admin_group" "/api/v1/admin/category"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "public_group" "/api/v1/categories-page"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "authorization_admin_group" "/api/v1/admin/stream"}}
      {{ template "backend" $StreamRegistry}},
      {{ template "authorization_admin_group" "/api/v1/admin/stream/close"}}
      {{ template "backend" $StreamRegistry}}
]
}
