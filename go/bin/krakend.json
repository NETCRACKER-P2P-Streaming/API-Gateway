{
  {{ $AmazonCognitoAdapter :="localhost:9090" }}
  {{ $StreamRegistry := "localhost:8080" }}
  "version": 2,
  "name": "{{ .service.name }}",
  "port": {{ .service.port }},
  "extra_config": {{ include "cors.txt" }},
  "endpoints": [
      {{ template "public_group" "/api/v1/registration"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/{username}"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/reset-password"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/streamers"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/users/file"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/login"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/refresh-token"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "public_group" "/api/v1/auth/logout"}}
      {{ template "backend" $AmazonCognitoAdapter}},

      {{ template "authorization_admin_group" "/api/v1/admin/grant-authority"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/users-in-group"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/disable-user"}}
      {{ template "backend" $AmazonCognitoAdapter}},
      {{ template "authorization_admin_group" "/api/v1/admin/enable-user"}}
      {{ template "backend" $AmazonCognitoAdapter}}
  ]
}